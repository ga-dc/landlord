  <input type='text' monthlyrent='monthly_rent' placeholder="Monthly Rent">
  <input type='text' sqft='sqft'placeholder="Square feet">
  <input type='text' numbeds='num_beds' placeholder="Number of Bedrooms">
  <input type='text' numbaths='num_baths' placeholder="Number of Baths">
  <input type='submit'>
</form>
