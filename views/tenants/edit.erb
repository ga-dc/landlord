<h2>Edit #<%= @tenant.id %> - <%= @tenant.name %> </h2>
<form action="/tenants/<%= @tenant.id %>" method="post">
  <input type="hidden" name="_method" value="put">

  <label for="tenant[name]">Name:</label>
  <input name="tenant[name]" value="<%= @tenant.name %>">

  <label>Age:</label>
  <input name="tenant[age]" value="<%= @tenant.age %>">

  <label>Gender:</label>
  <input name="tenant[gender]" value="<%= @tenant.gender %>">

  <label>Apartment Id:</label>
  <input name="tenant[apartment_id]" value="<%= @tenant.apartment_id %>">

  <input type="submit" value="Update">
</form>
