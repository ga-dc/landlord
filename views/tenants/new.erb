<h2>New Tenant</h2>


  <form action="/tenants" method="post">
    <label>Name:</label>
    <input name="tenant[name]">

    <label>Age:</label>
    <input name="tenant[age]">

    <label>Gender:</label>
    <input name="tenant[gender]">

    <label>Apartment ID:</label>
    <select name="tenant[apartment_id]">
      <% Apartment.all.each do |apt|%>
      <option value="<%= apt.id%>"><%= apt.address%></option>
      <% end%>
    </select>
    <!-- <input id="apt_field" type="hidden" name="tenant[apartment_id]" value> -->

    <input type="submit" value="Create">

  </form>
